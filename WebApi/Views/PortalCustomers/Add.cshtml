@using Entities.Concrete
@model Customer


<form method="post" asp-action="Add" >
    <div class="mb-3">
  <input type="hidden" asp-for="Company" value="portal" class="form-control" >
  <label for="FirstName" class="form-label">First Name</label>
  <input type="text" asp-for="FirstName" class="form-control" >
  <label for="LastName" class="form-label">Last Name</label>
  <input type="text" asp-for="LastName" class="form-control" >
  <label for="NationalityId" class="form-label">National Identity</label>
  <input type="text" asp-for="NationalityId" class="form-control" >
  <label for="DateOfBirth" class="form-label">Date Of Birth</label>
  <input type="text" asp-for="DateOfBirth" class="form-control" >
</div>

<button type="submit">Add</button>
</form>